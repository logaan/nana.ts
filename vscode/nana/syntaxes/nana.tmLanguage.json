{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Nana",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.nana",
					"match": "\\b([A-Z][a-z]*)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.nana",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.nana",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.nana"
}